# Test ParliamentRdfNode.

@prefix p: <http://purl.org/pipeline/ont#> .
@prefix : <http://localhost/node/> .

# Root location (as local name) for ParliamentRdfNodes on server ":"
# (which is defined above as http://localhost/node/):
p:ParliamentRdfNode p:root ( : <http://localhost:8080/parliament/> ) .
# p:ParliamentRdfNode p:root ( : "$PARLIAMENT_ROOT" ) .

:graph1 a p:ParliamentRdfNode ;
	p:inputs ( "infile.txt" ) ;
	p:updater "noop.sparql" .

:graph2 a p:ParliamentRdfNode ;
	p:inputs ( :graph1 ) ;
	p:updater "three-updater.sparql" .

:graph3 a p:ParliamentRdfNode ;
	p:inputs ( :graph2 ) ;
	p:updater "noop.sparql" .

